
<div class="modal fade" id="clubModal" tabindex="-1" role="dialog"
	aria-labelledby="clubModalLabel" aria-hidden="true">
	<div class="modal-dialog" role="document">
		<div class="modal-content">
			<div class="modal-header">
				<h5 class="modal-title" id="exampleModal3Label">{{model.club.name}}</h5>
				<button type="button" class="close" data-dismiss="modal"
					aria-label={{t "forms.close"}}>
					<span aria-hidden="true">&times;</span>
				</button>
			</div>
			<div class="modal-body">
				<form>
					<div class="form-group row">
						<label for="inputId" class="col-sm-2 col-form-label">{{t
							"forms.common.id"}}</label>
						<div class="col-sm-10">{{input class="form-control"
							id="inputId" value=model.club.id placeholder=(t
							"forms.common.id") disabled=true}}</div>
					</div>
					<div class="form-group row">
						<label for="inputName" class="col-sm-2 col-form-label">{{t
							"forms.common.name"}}</label>
						<div class="col-sm-10">{{input class="form-control"
							id="inputName" value=model.club.name placeholder=(t
							"forms.common.name")}}</div>
					</div>
					<div class="form-group row">
						<label for="inputEmail" class="col-sm-2 col-form-label">{{t
							"forms.common.email"}}</label>
						<div class="col-sm-10">{{input class="form-control"
							id="inputEmail" value=model.club.emailAddress placeholder=(t
							"forms.common.email")}}</div>
					</div>
					<div class="form-group row">
						<label for="inputWebUrl" class="col-sm-2 col-form-label">{{t
							"forms.common.webSiteUrl"}}</label>
						<div class="col-sm-10">{{input type="text"
							class="form-control" id="inputWebUrl" value=model.club.webSiteUrl
							placeholder=(t "forms.common.webSiteUrl")}}</div>
					</div>
					<div class="form-group row">
						<label for="inputFacebookPage" class="col-sm-2 col-form-label">{{t
							"forms.common.facebookPage"}}</label>
						<div class="col-sm-10">{{input class="form-control"
							id="inputFacebookPage" value=model.club.facebookPage
							placeholder=(t "forms.common.facebookPage")}}</div>
					</div>

					<div class="modal-footer">
						<button type="button" class="btn btn-secondary"
							data-dismiss="modal">Close</button>
						<button id="saveButton" class="btn btn-primary"
							{{action "save"}} >{{t "user.save"}}</button>
					</div>
				</form>
			</div>
		</div>

	</div>
</div>
	{{outlet}}